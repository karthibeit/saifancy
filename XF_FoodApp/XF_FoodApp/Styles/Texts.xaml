    <!--#region FONTSFAMILY [ProductSans]-->
    <!--#region Bold-->
    <OnPlatform x:Key="BoldFont_ProductSans" x:TypeArguments="x:String">
        <On Platform="Android" Value="FontsFamily/ProductSans-Bold.ttf#ProductSans" />
        <On Platform="iOS" Value="ProductSans-Bold" />
        <On Platform="UWP" Value="Assets/Fonts/ProductSans-Bold.ttf#ProductSans" />
    </OnPlatform>
    <!--#endregion-->

    <!--#region ItalicBold-->
    <OnPlatform x:Key="BoldItalicFont_ProductSans" x:TypeArguments="x:String">
        <On Platform="Android" Value="FontsFamily/ProductSans-BoldItalic.ttf#ProductSans" />
        <On Platform="iOS" Value="ProductSans-BoldItalic" />
        <On Platform="UWP" Value="Assets/Fonts/ProductSans-BoldItalic.ttf#ProductSans" />
    </OnPlatform>
    <!--#endregion-->

    <!--#region Italic-->
    <OnPlatform x:Key="ItalicFont_ProductSans" x:TypeArguments="x:String">
        <On Platform="Android" Value="FontsFamily/ProductSans-Italic.ttf#ProductSans" />
        <On Platform="iOS" Value="ProductSans-Italic" />
        <On Platform="UWP" Value="Assets/Fonts/ProductSans-Italic.ttf#ProductSans" />
    </OnPlatform>
    <!--#endregion-->

    <!--#region Regular-->
    <OnPlatform x:Key="RegularFont_ProductSans" x:TypeArguments="x:String">
        <On Platform="Android" Value="FontsFamily/ProductSans-Regular.ttf#ProductSans" />
        <On Platform="iOS" Value="ProductSans-Regular" />
